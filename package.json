    "@reduxjs/toolkit": "^1.8.1",
    "async-mutex": "^0.3.2",
    "formik": "^2.2.9",
    "ky": "^0.30.0",
    "localforage": "^1.10.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "6",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "twin.macro": "rc",
    "urlcat": "^2.0.4"
