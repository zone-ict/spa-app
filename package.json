    "jsdom": "^19.0.0",
    "lint-staged": ">=10",
    "msw": "^0.41.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.0.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-react-svg": "^0.2.0",
    "vitest": "^0.12.9",
    "whatwg-fetch": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "vitest related --watch false"
    ],
    "*.{js,jsx,ts,tsx,json,html,md,css}": [
      "prettier --write"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
